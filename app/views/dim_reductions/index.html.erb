<p id="notice"><%= notice %></p>

<h1>Dim Reductions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Label</th>
      <th>Description</th>
      <th>Speed</th>
      <th>Link</th>
      <th>Attr_json</th>
      <th>Command line</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dim_reductions.each do |d| %>
      <tr>
	<td><%= d.name %></td>
        <td><%= d.label %></td>
        <td><%= d.description %></td>
	<td><%= d.speed_id %></td>
        <td><%= d.link %></td>
        <td><%= d.attrs_json %></td>
        <td><%= d.program %></td>
        <td><%= link_to 'Show', d %></td>
        <td><%= link_to 'Edit', edit_dim_reduction_path(d) %></td>
        <td><%= link_to 'Destroy', d, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dim Reduction', new_dim_reduction_path %>
